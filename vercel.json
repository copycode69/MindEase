{
  "version": 2,
  "builds": [
    { "src": "frontend/**/*", "use": "@vercel/static" },
    { "src": "public/**/*", "use": "@vercel/static" }
  ],
  "routes": [
    {
      "src": "/(logo\\.png|jay\\.jpg|.*\\.(png|jpg|jpeg|svg|webp|gif|css|js|ico))",
      "dest": "/public/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend/$1"
    }
  ]
}
